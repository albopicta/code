  .text
  .globl main
main:      addi $sp $sp 8
           sw $s0 0($sp)
           sw $ra 4($sp)
           addi $a0 $0 1
           jal leaf
leaf:      addi $s0 $a0 1      # result = f+1
           slti $t0 $a0 6      # $t1 = f < 6 or f<=5
           bne $t0 $0 output   # when that's not met
           add $a0 $s0 $0
           jal leaf 
output:    add $v0 $s0 $0      # return result
exit:      lw $ra 4($sp)
           lw $s0 0($sp)
           addi $sp $sp -4
           syscall
